    <select id="informationColumn" resultMap="tableColumn">
        SELECT
            a.table_name,
            a.table_comment,
            b.column_name,
            b.column_comment
        FROM
            information_schema.TABLES a
            LEFT JOIN information_schema.COLUMNS b ON a.table_name = b.TABLE_NAME
        WHERE
            a.table_schema = 'test'
        ORDER BY
            a.table_name
    </select>

